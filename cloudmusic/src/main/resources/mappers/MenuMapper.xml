<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace='com.example.cloudmusic.dao.MenuDao'>
	<resultMap id="MenuResultMap" type="com.example.cloudmusic.entity.Menu">
		<id column="id" jdbcType="INTEGER" property="id" />
		<result column="menuCode" jdbcType="VARCHAR" property="menuCode" />
		<result column="menuName" jdbcType="VARCHAR" property="menuName" />
		<result column="supIndex" jdbcType="INTEGER" property="supIndex" />
	</resultMap>

	<select id="getMenu" parameterType="java.lang.Integer" resultMap="MenuResultMap">
		select id,menuCode,menuName,supIndex from menu where supIndex = #{supIndex}
	</select>
</mapper>